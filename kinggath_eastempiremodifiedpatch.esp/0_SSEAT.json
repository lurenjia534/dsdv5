  {
    "editor_id": "kgEEP_DungeonClaimExplanation_Off",
    "form_id": "06000005|kinggath_eastempiremodifiedpatch.esp",
    "index": 1,
    "type": "MESG DESC",
    "original": "The EEC will no longer lay claim to new dungeons after an East Empire Relic is used within.",
